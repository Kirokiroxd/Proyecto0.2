@model Empleado

<h2>Registrar Empleado</h2>

<form asp-action="Create" method="post">
    <input asp-for="Nombre" class="form-control mb-2" placeholder="Nombre" />
    <input asp-for="Apellido" class="form-control mb-2" placeholder="Apellido" />
    <input asp-for="Email" class="form-control mb-2" placeholder="Email" />
    <input asp-for="Posicion" class="form-control mb-2" placeholder="Posición" />

    <button class="btn btn-success">Guardar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
